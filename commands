#!/bin/bash
set -e
APP_CMD="$1"
APP_NAME="$2"

APP_PATH="/home/dokku/$APP_NAME"

VHOST_PATH="$APP_PATH/VHOST"
URL_PATH="$APP_PATH/VHOST"
PORT_PATH="$APP_PATH/VHOST"
HEAD_PATH="$APP_PATH/VHOST"
CONTAINER_PATH="$APP_PATH/VHOST"
NGINX_CONF_PATH="$APP_PATH/nginx.conf"

cat "echo $APP_CMD $APP_NAME" >> log.txt